<%= render_to_element('#strength-label', partial: 'password_strength_label', outer: true, locals: { score: @score, length: @length }) %>
<%= render_to_element('#strength-bar', partial: 'password_strength', outer: true, locals: { score: @score }) %>
<%= raw("document.querySelector('#submit-password').disabled = #{@score < 2 || @length < 8};") %>
